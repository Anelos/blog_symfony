<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8"/>
    <title>{% block title %}Welcome!{% endblock %}</title>
    {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('assets/css/bootstrap.min.css') }}">
        <style>
            .navbar {
                border-radius: 0%;
            }
            .navbar-header {
                float: left;
                padding: 10px;
                text-align: center;
                width: 100%;
            }
            .navbar-brand {
                float: none;
            }
        </style>
    {% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/>
    <script src="{{ asset('assets/js/jquery-3.2.1.min.js') }}"></script>
    <script src="https://use.fontawesome.com/d45ea48418.js"></script>
</head>
<body>
{{ include('component/navbar.html.twig') }}
{% block body %}{% endblock %}
